= javascript_include_tag 'jQuery.dataTables.min'

.container
  .row
    .col-md-12
      .admin
        %h1 Admin page
        %h4 Videos
        %table#videos
          %thead
            %tr
              %th Title
              %th URL
              %th Presenter
          %tbody
            -@videos.each do |video|
              %tr
                %td= video.title
                %td= video.url
                %td= video.speaker_name